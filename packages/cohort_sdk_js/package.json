{
  "name": "@kindredgroup/cohort_sdk_js",
  "version": "0.0.1",
  "author": "Kindredgroup",
  "license": "UNLICENSED",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "cohort_sdk_js",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "universal-apple-darwin"
      ]
    }
  },
  "dependencies": {
    "@napi-rs/cli": "^2.16.0"
  },
  "devDependencies": {
    "ava": "^4.3.3",
    "uuid": "^9.0.0",
    "esbuild": "^0.18.0"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "dist/talos/**",
    "index.d.ts",
    "index.js",
    "cohort_sdk_js*",
    "scripts/postinstall.sh"
  ],
  "scripts": {
    "artifacts": "napi artifacts -d .",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "test": "ava",
    "postinstall": "scripts/postinstall.sh",
    "universal": "napi universal",
    "version": "napi version",
    "prepack": "scripts/bundle-talos.sh",
    "_prepublishOnly": "napi prepublish -t npm && esbuild --minify --outfile=index.js --allow-overwrite index.js"
  },
  "packageManager": "yarn@3.3.1",
  "optionalDependencies": {
    "cohort_sdk_js-win32-x64-msvc": "0.0.1",
    "cohort_sdk_js-darwin-x64": "0.0.1",
    "cohort_sdk_js-linux-x64-gnu": "0.0.1",
    "cohort_sdk_js-darwin-arm64": "0.0.1",
    "cohort_sdk_js-darwin-universal": "0.0.1"
  }
}
